<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ga.calculadora.calculadoradecombustivelga">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ga"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ga_round"
        android:supportsRtl="true"
        android:theme="@style/GaTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimplesActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".SimplesResultadoActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".AvancadoActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".AvancadoResultadoActivity" />
        <activity android:name=".ExtraActivity"></activity>
    </application>

</manifest>